            <JFXTextField fx:id="searchText" layoutX="28.0" layoutY="119.0" prefHeight="25.0" prefWidth="207.0" />
            <JFXTextField fx:id="startPage" layoutX="28.0" layoutY="49.0" prefHeight="25.0" prefWidth="207.0" />
            <Text layoutX="28.0" layoutY="111.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Text to search">
